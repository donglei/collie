{
    "name": "collied", 
    "description": "A network framework written in D.", 
    "homepage": "http://collied.org/", 
    "copyright": "Copyright Â© 2016, collied.org", 
    "authors": [
        "Putao's Collie team."
    ], 
    "versions" : ["TCP_POOL"],
    "dependencies": {
        "experimental_allocator": {
            "path": "experimental_allocator"
        }
    }, 
    "configurations": [
        {
            "name": "default", 
            "targetType": "library"
        }, 
        {
            "name": "ssl", 
            "targetType": "library", 
            "versions": [
                "SSL"
            ], 
            "dependency": {
                "openssl": "~>1.1.4+1.0.1g"
            }
        }, 
        {
            "name": "exe", 
            "targetType": "executable", 
            "versions": [
                "EXEc"
            ], 
            "dependency": {
                "openssl": "~>1.1.4+1.0.1g"
            }
        }, 
        {
            "name": "exessl", 
            "targetType": "executable", 
            "versions": [
                "SSL", 
                "EXEc"
            ], 
            "dependency": {
                "openssl": "~>1.1.4+1.0.1g"
            }
        }
    ], 
    "buildTypes": {
        "debug": {
            "buildOptions": [
                "debugMode", 
                "debugInfo", 
                "profile"
            ]
        }, 
        "release": {
            "buildOptions": [
                "releaseMode", 
                "inline", 
                "optimize"
            ]
        }
    }
}
